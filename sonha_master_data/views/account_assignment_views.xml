<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="account_assignment_tree_view" model="ir.ui.view">
            <field name="name">account.assignment.tree.view</field>
            <field name="model">account.assignment</field>
            <field name="arch" type="xml">
                <tree string="Nhóm định khoản doanh thu" editable="bottom">
                    <field name="code"/>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="account_assignment_action" model="ir.actions.act_window">
            <field name="name">Nhóm định khoản doanh thu</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">account.assignment</field>
            <field name="view_mode">tree</field>
        </record>
    </data>
</odoo>