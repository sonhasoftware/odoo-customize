<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="master_data_report_tree_view" model="ir.ui.view">
        <field name="name">master.data.report.tree.view</field>
        <field name="model">master.data.report</field>
        <field name="arch" type="xml">
            <tree string="Bảng chấm công thô">
                <field name="employee_id"/>
                <field name="department_id"/>
                <field name="attendance_time"/>
                <field name="month"/>
            </tree>
        </field>
    </record>

    <record id="master_data_report_action" model="ir.actions.act_window">
        <field name="name">Bảng công thô</field>
        <field name="res_model">master.data.report</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>